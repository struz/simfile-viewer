{
  "name": "simfile-viewer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service build --mode development --watch",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
