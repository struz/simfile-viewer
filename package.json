    "vuetify": "^1.5.5"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.4",
    "@types/node": "^11.13.8",
    "@vue/cli-plugin-babel": "^3.6.0",
